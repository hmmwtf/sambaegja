<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.BoardDAO">
	<select id="select" resultType="BoardVO">
		select * from board
	</select>
	<select id="selectBoardByIdx" parameterType="int" resultType="BoardVO">
		select * from board where idx=#{idx}
	</select>
	<insert id="insert" parameterType="BoardVO">
		insert into board(idx,title,content,likey,category,regdate,id) values(0,date_format(#{title},'%Y-%m-%d'),#{content},#{likey},#{category},#{regDate},#{id})
	</insert>
</mapper>  
